{
  "private": true,
  "name": "aimer-manager",
  "version": "0.0.0",
  "scripts": {
    "test": "lerna bootstrap && npm run lint && lerna exec -- npm test",
    "lint": "xo"
  },
  "xo": {
    "extends": "rem/prettier",
    "ignores": [
      "**/example/**"
    ],
    "envs": [
      "browser"
    ],
    "rules": {
      "unicorn/filename-case": 0
    }
  },
  "devDependencies": {
    "eslint-config-rem": "^3.2.0",
    "lerna": "^2.4.0",
    "xo": "^0.18.2"
  }
}
